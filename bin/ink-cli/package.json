{
	"name": "ink-cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"start": "node dist/cli.js",
		"run": "npm run build && npm start",
		"postinstall": "npm run build"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@mishieck/ink-titled-box": "^0.3.0",
		"ink": "^6.3.1",
		"ink-ascii": "^0.0.4",
		"ink-big-text": "^2.0.0",
		"ink-chart": "^0.1.1",
		"ink-color-pipe": "^5.0.0",
		"ink-confirm-input": "^2.0.0",
		"ink-divider": "^4.1.1",
		"ink-form": "^2.0.1",
		"ink-gradient": "^3.0.0",
		"ink-link": "^5.0.0",
		"ink-markdown": "^1.0.4",
		"ink-multi-select": "^2.0.0",
		"ink-picture": "^1.3.1",
		"ink-progress-bar": "^3.0.0",
		"ink-quicksearch-input": "^1.0.0",
		"ink-select-input": "^6.2.0",
		"ink-spinner": "^5.0.0",
		"ink-syntax-highlight": "^2.0.2",
		"ink-tab": "^5.2.0",
		"ink-table": "^3.1.0",
		"ink-task-list": "^2.0.0",
		"ink-text-input": "^6.0.0",
		"meow": "^11.0.0",
		"react": "^19.1.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^19.2.2",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"resolutions": {
		"react": "^19.1.0",
		"react-reconciler": "^0.32.0"
	}
}
