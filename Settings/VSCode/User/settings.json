{
  "workbench.colorTheme": "Behave",
  "workbench.iconTheme": "vs-seti",
  "search.followSymlinks": false,
  "workbench.colorCustomizations": {
    "[Behave]": {
      "sideBar.background": "#201c1c",
      "editor.background": "#002133"
    }
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "files.exclude": {
    "**/node_modules/**": true,
    "**/.DS_Store": true,
    "**/__generated__": true,
    "**/.git": true,
    "**/bundle/ruby/**": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/__generated__": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/__generated__": true,
    "**/bundle/ruby/**": true
  },
  "window.zoomLevel": 2,
  "editor.fontSize": 14,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "blink",
  "explorer.openEditors.visible": 6,
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": "explicit",
    "source.fixAll": "explicit"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "editor.inlineSuggest.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "redhat.telemetry.enabled": true,
  "npm.packageManager": "yarn",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.detectIndentation": false,
  "git.confirmSync": false,
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.renderWhitespace": "all",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "git.enableSmartCommit": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.formatOnSave": true
  },
  "editor.accessibilitySupport": "off",
  "npm.keybindingsChangedWarningShown": true,
  "xml.server.binary.trustedHashes": [
    "1facb3e258f78f295a988276a27ddd28465b10ab2ce9d81a07f9decee26abd39"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "sync.quietSync": true,
  "editor.suggestSelection": "first",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.largeFileOptimizations": false,
  "terminal.integrated.inheritEnv": false,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoSearchPaths": false,
  "python.analysis.autoImportUserSymbols": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "python.defaultInterpreterPath": "/Users/hemantv/anaconda3/envs/tf",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "github.copilot.editor.enableAutoCompletions": true,
  "git.openRepositoryInParentFolders": "always",
  "git.ignoreLimitWarning": true,
  "typescript.tsserver.maxTsServerMemory": 4096
}
